public class q13a{
        public static void main(String[] args) {
            int x = 5;
            int y;
    
            if (x > 0) {
                y = 1;
            } else if (x < 0) {
                y = -1;
            } else {
                y = 0;
            }
    
            System.out.println("y = " + y);
        }
    
}
